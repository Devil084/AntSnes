#include "AntSnes.hrh"
#include "AntSnes.loc"
#include <appinfo.rh>
#include <AntSnes.rsg>

UID2 KUidAppRegistrationResourceFile
UID3 _UID3

RESOURCE APP_REGISTRATION_INFO
	{
	app_file="AntSnes";
	// no extension used, framework selects the correct version (.r01, .r03)
	localisable_resource_file =  "\\resource\\apps\\AntSnes";
	localisable_resource_id = R_LOCALISABLE_APP_INFO;

	embeddability=KAppNotEmbeddable;
	newfile=KAppDoesNotSupportNewFile;
	}

